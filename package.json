{
  "name": "pollin8-web-client",
  "version": "1.0.0",
  "description": "Web client for the Pollin8 simulation engine",
  "author": "Tom Saleeba",
  "private": true,
  "scripts": {
    "dev": "nuxt",
    "build": "nuxt build",
    "start": "nuxt start",
    "test": "ava",
    "test:watch": "ava --watch",
    "generate": "nuxt generate"
  },
  "dependencies": {
    "@mdi/font": "4.7.95",
    "@nuxtjs/axios": "^5.4.1",
    "@nuxtjs/proxy": "^1.3.3",
    "bootstrap": "^4.1.3",
    "bootstrap-vue": "^2.0.0-rc.11",
    "chart.js": "^2.8.0",
    "cross-env": "^5.2.0",
    "leaflet-draw": "^1.0.4",
    "leaflet-sleep": "https://github.com/ternandsparrow/Leaflet.Sleep",
    "moment": "^2.24.0",
    "nuxt": "^2.4.0",
    "nuxt-env": "^0.1.0",
    "nuxt-leaflet": "^0.0.18",
    "nuxt-rollbar-module": "^1.0.1",
    "socket.io-client": "^2.2.0",
    "vue-chartjs": "^3.4.2",
    "vue-izitoast": "^1.1.2",
    "vue-scrollto": "^2.15.0",
    "vue-vimeo-player": "0.0.10",
    "vuetify": "^1.5.0",
    "vuetify-loader": "^1.2.0"
  },
  "devDependencies": {
    "@babel/register": "^7.4.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "ava": "^1.4.1",
    "babel-plugin-module-resolver": "^3.2.0",
    "browser-env": "^3.2.6",
    "nodemon": "^1.18.9",
    "require-extension-hooks": "^0.3.3",
    "require-extension-hooks-babel": "^1.0.0-beta.1",
    "require-extension-hooks-vue": "^2.0.0",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.2"
  },
  "ava": {
    "require": [
      "@babel/register",
      "./test/setup.js"
    ],
    "files": [
      "./test/specs/*.js"
    ],
    "snapshotDir": "./test/snapshots"
  },
  "babel": {
    "presets": [
      "@babel/env"
    ],
    "plugins": [
      [
        "module-resolver",
        {
          "alias": {
            "@": "./",
            "~": "./"
          }
        }
      ]
    ]
  }
}
